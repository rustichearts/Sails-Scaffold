    "font-awesome-animation": "~0.0.5",
    "lodash": "~2.4.1",
    "kazitori.js": "~1.0.3",
    "angular": "~1.2.25",
    "angular-animate": "~1.2.25",
    "bootstrap": "~3.3.1",
    "angular-bootstrap": "~0.12.0",
